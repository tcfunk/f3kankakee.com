{% set card_body %}
  {% include 'f3k3_radix:address' with {
    address_content: content.field_address,
    latlng: content.field_geolocation,
  } %}
  {{ content|without([
    'field_address',
    'field_geolocation',
    'field_logo',
    'field_title',
  ]) }}
{% endset %}


{% include 'f3k3_radix:card' with {
  card_image_overlays: false,
  card_title: label,
  card_title_tag: 'h3',
  card_body: card_body,
  card_media: content.field_logo,
  card_image_cap: 'top',
  card_link_url: url,
} %}